<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.learngerman">

    <application
        android:allowBackup="true"
        android:icon="@drawable/german"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Phrases"
            android:label="@string/phrases_activity"/>
        <activity android:name=".Colors"
            android:label="@string/colors_activity"/>
        <activity android:name=".Family"
            android:label="@string/fam_activity"/>
        <activity android:name=".Numbers"
            android:label="@string/numbers_activity"
       />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>